/*class=list的table属性*/
.list {
	font-family:Arial,Helvetica,sans-serif;
	border-collapse:collapse;
	margin: auto;
	width:75%
}
.list td, .list th {
	border: 1px solid #ddd;
	padding: 8px;
}
.list tr:nth-child(even){backgrout-color: #f2f2f2;}
.list tr:hover {background-color: #ddd;}
.list th {
	padding-top: 12px;
	padding-bottom: 12px;
	text-align: center;
	background-color: #4CAF50;
	color: white;
}
/*class=select的table属性*/
.select {
	font-family:Arial,Helvetica,sans-serif;
	margin: auto;
	width: 75%;
	border-style: outset;
	border-color: #4CAF50;
	background-color: #cccccc;
}
.select th, .select td {
	border: 0;
	text-align: center;
}

